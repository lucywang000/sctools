{
  "name": "sctools",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch": "shadow-cljs watch --debug :app",
    "test": "shadow-cljs compile :test",
    "ci-build": "shadow-cljs release :ci",
    "ci-test": "karma start --single-run",
    "compile-css": "yarn postcss --verbose static/styles/tailwind/tailwind-main.css static/styles/tailwind/tailwind-custom.css --dir assets/app/static/dev/styles/",
    "watch-css": "yarn compile-css --watch",
    "purge-fa-css": "yarn postcss resources/app/static/fontawesome/css/all.min.css -o resources/app/static/fontawesome/css/fa.purged.css --env prod",
    "release-js": "yarn shadow-cljs release :app",
    "release-tailwind-css": "yarn postcss --verbose static/styles/tailwind/tailwind-main.css static/styles/tailwind/tailwind-custom.css --dir resources/app/static/styles/ --env prod",
    "release-css": "yarn release-tailwind-css; yarn purge-fa-css",
    "release": "yarn release-css; yarn release-js",
    "restart-js": "goreman run restart js",
    "start": "goreman start"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@testing-library/react": "^10.0.4",
    "autoprefixer": "^9.7.6",
    "create-react-class": "^15.6.3",
    "cssnano": "^4.1.10",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cljs-test": "^0.1.0",
    "marked": "^1.1.0",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "2.1.1",
    "shadow-cljs": "2.9.6",
    "source-map-support": "^0.5.13",
    "stylelint": "^11.1.1",
    "tailwindcss": "^1.4.6",
    "ws": "^7.1.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.4",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "@popperjs/core": "^2.4.0",
    "highlight.js": "^10.0.3",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-devtools-core": "^4.2.0",
    "react-dom": "^16.13.1",
    "react-highlight.js": "1.0.7",
    "react-refresh": "^0.8.1",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.4.1",
    "xregexp": "^4.2.4"
  }
}
